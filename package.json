{
  "name": "react-infinitescroll",
  "version": "0.0.1",
  "description": "Infinite scroll component",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "build": "babel src --out-dir lib --ignore __tests__",
    "build:umd": "webpack src/index.js dist/react-infinitescroll.js && NODE_ENV=production webpack src/index.js dist/react-infinitescroll.min.js",
    "lint": "eslint src examples",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanSnow/react-infinitescroll.git"
  },
  "keywords": [
    "react",
    "infinite-scroll"
  ],
  "author": "DanSnow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DanSnow/react-infinitescroll/issues"
  },
  "homepage": "https://github.com/DanSnow/react-infinitescroll",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.11.11",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.0",
    "jasmine-enzyme": "^2.1.0",
    "jest-cli": "^18.1.0",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "rimraf": "^2.5.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "decko": "^1.1.3",
    "lodash": "^4.14.0",
    "react": "^15.2.1",
    "react-event-listener": "^0.4.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "node_modules/jasmine-enzyme/lib/jest.js",
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme"
    ]
  }
}
